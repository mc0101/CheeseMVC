@model List<CheeseMVC.Models2.CheeseCategory>

    




<h1>Categories</h1>

<p><a asp-controller="Category" asp-action="Add">Add Category</a></p>

@if (Model.Count == 0)
{
   <p>No categories yet!</p> 
}

   
       @foreach (var category in Model)
    {
       <ul>
           <li>@category.Name</li>
          
       </ul>
}


   
